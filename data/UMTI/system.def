; Definitions of system screens

;----------------------------
[Info]
name = "Ultimate Mugen Team"          ;Name of motif
author = "Saikou Shiva"       ;Motif author name
versiondate = 05,24,2023  ;Version date of motif (MM-DD-YYYY) (new in MUGEN 2009)
mugenversion = 1.0        ;Version of M.U.G.E.N motif is compatible with (new in MUGEN 2009)
localcoord = 960,720      ;Local coordinate space width and height (new in MUGEN 2009)

;----------------------------
;Directories are checked in this order:
;1. current directory system.def is in
;2. data/
;3. your MUGEN directory
;eg. If you write "spr = system.sff", and this system.def file
;    is in ~/mugen/data/mymotif/, then Mugen will first look
;    for ~/mugen/data/mymotif/system.sff. If it does not find
;    it there, it looks for ~/mugen/data/system.sff (the default).
;    Finally, if that is not found, it will look for
;    ~/mugen/system.sff.

[Files]
spr = system.sff
snd = system.snd
logo.storyboard = data/Intro.def
intro.storyboard = -1
select = select.def
fight = data/VPHD_Bars/fightHD_UMT.def
font1 = f-4x6.def
font2 = f-6x9.def
font3 = menu.def
font4 = Open_Sans.def
font5 = default-3x5-bold.def
font6 = airstrikecond.def
font7 = stage_sel.def
font8 = name.def
module =                  ;Ikemen feature: External Lua code

