; Definition file for Kung Fu Man
; Contains all the filenames needed for the character

; Player information
[Info]
name = "Sogiita Gunha"        ;Name of character
displayname = "Sogiita Gunha (WIP)" ;Name of character to display
versiondate = 08,01,2024   ;Version date of character (MM-DD-YYYY or X.XX)
mugenversion = 1.1         ;Version of M.U.G.E.N character works on (X.XX)
author = "DidiGetsRekt"         ;Character author name
pal.defaults = 1,2,3,4      ;Default palettes in order of preference (up to 4)
                            ;Numbering starts from 1
localcoord = 320,240        ;Local coordinate space width and height

; Files for the player
[Files]
cmd     = Gunha.cmd           ;Command set
cns     = Gunha.cns            ;Constants
st      = Gunha.cns           ;States
st2	= Gunha_N.cns
st3	= Gunha_S.cns
st4	= Gunha_H.cns
st5	= Gunha_Ex.cns
st6	= Gunha_Helper.cns
stcommon = common.cns      ;Common states (from data/ or motif)
sprite  = Gunha.sff          ;Sprite
anim    = Gunha.air           ;Animation
sound   = Gunha.snd            ;Sound
ai      = Gunha.ai            ;AI hints data (not used)
movelist= Gunha_Movelist.dat

; Maps character selection buttons to palette numbers.
[Palette Keymap]
x = 1 ;Press button X to select palette 1, etc.
y = 2
z = 3
a = 4
b = 5
c = 6
;x2 = 7 ;Hold Start and press button X for palette 7, etc.
;y2 = 8
;z2 = 9
;a2 = 10
;b2 = 11
;c2 = 12

; Arcade mode
[Arcade]
intro.storyboard = 
ending.storyboard = 
