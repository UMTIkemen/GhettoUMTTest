value = 5150

[State 5110, 12]
type = VarSet
trigger1 = SysVar(0)
trigger1 = Time = 0
sysvar(0) = 0

[State 5110, 13] ;Friction
type = VelMul
trigger1 = 1
x = 0.85

[State 5110, ChangeState]
type = ChangeState
triggerall = alive
trigger1 = time =30
value = 15120
ctrl = 0

