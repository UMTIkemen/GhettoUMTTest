;---------------------------------------------------------------------------
; Watchdog Helper
[Statedef 179]
type = S
movetype = I
physics = N
velset = 0,0
anim = 9741

[State Check for height]
type = Null
trigger1 = 1 || fvar(31) := gameheight - 240.0

;=======CHECK FOR TARGETS======

[State Default]
type = Null
trigger1 = 1 || var(31) := -1

[State Check if player has a target]
type = Null
triggerall = root,numtarget
trigger1 = 1 || var(31) := (root,cond(1,(target,cond(1,ID,1)),1)) ;Save its ID.

;=======RESETS=======
[State Reset G. B]
type = Varset
trigger1 = var(31) = -1
trigger2 = !playerIDExist(var(31))
trigger3 = playerIDExist(var(31))
trigger3 = playerID(var(31)),stateno != 5242
;Keep the Wallbounce Stick and Wall bounce timers, reset the ground bounce one.
var(30) = ((((var(30)/4)/4)%4)*4*4 + ((var(30)/4)%4)*4 + 0)

[State Reset W. B]
type = Varset
trigger1 = var(31) = -1
trigger2 = !playerIDExist(var(31))
trigger3 = playerIDExist(var(31))
trigger3 = !((playerID(var(31)),stateno = 5252) || (playerID(var(31)),stateno = 3015))
;Keep the Wallbounce Stick, reset the wallbounce one, keep ground bounce.
var(30) = ((((var(30)/4)/4)%4)*4*4 + 0 + var(30)%4)

[State Reset W. B Sticky]
type = Varset
trigger1 = var(31) = -1
trigger2 = !playerIDExist(var(31))
trigger3 = playerIDExist(var(31))
trigger3 = !((playerID(var(31)),stateno = 5257) || (playerID(var(31)),stateno = 3011))
;Keep the wall bounce, and groundbounce, reset the wall stick
var(30) = 0 + var(30)%16

;=======DETECT GROUND BOUNCE======

[State Detect if player hits a groundbounce]
type = Null
triggerall = var(31) != -1
trigger1 = playerIDExist(var(31))
trigger1 = playerID(var(31)),stateno = 5242 && playerID(var(31)),time = [0,1]
trigger1 = (var(30)%4) = 0
;Keep the Wallbounce Stick and Wall bounce timers, reset the ground bounce one.
trigger1 = 1 || var(30) := ((((var(30)/4)/4)%4)*4*4 + ((var(30)/4)%4)*4 + 1)

;======DETECT WALL BOUNCE======

[State Detect if player hits a wallbounce]
type = Null
triggerall = var(31) != -1
trigger1 = playerIDExist(var(31))
trigger1 = ((playerID(var(31)),stateno = 5252) || (playerID(var(31)),stateno = 3015)) && playerID(var(31)),time = [0,1]
trigger1 = (var(30)/4)%4 = 0
;Keep the Wallbounce Stick, reset the wallbounce one, keep ground bounce.
trigger1 = 1 || var(30) := ((((var(30)/4)/4)%4)*4*4 + 4 + var(30)%4)

;======DETECT WALL STICK======

[State Detect if player hits a wallbounce sticky]
type = Null
triggerall = var(31) != -1
trigger1 = playerIDExist(var(31))
trigger1 = ((playerID(var(31)),stateno = 5257) || (playerID(var(31)),stateno = 3011)) && playerID(var(31)),time = [0,1]
trigger1 = ((var(30)/4)/4)%4 = 0
;Keep the wall bounce, and groundbounce, reset the wall stick
trigger1 = 1 || var(30) := 16 + var(30)%16


;groundbounce effects
[State 5100, 10]
