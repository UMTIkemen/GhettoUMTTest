[Statedef 40]
type    = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1

[State -3] ;←コマンドを入れたらVarをON
type = varset
trigger1 = var(26) = 0
trigger1 = command = "Ljump"
v = 26
value = 3

[State 40, 1]
type = VarSet
trigger1 = Time = 0
sysvar(1) = 0

[State 40, 2]
type = VarSet
trigger1 = command = "holdfwd"
sysvar(1) = 1

[State 40, 3]
type = VarSet
trigger1 = command = "holdback"
sysvar(1) = -1

[State 40, 4];通常ジャンプ（ただ上を押しただけ）
type = VelSet
trigger1 = var(26) = 0
trigger1 = AnimTime = 0
trigger1 = command = "holdup"
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x), const(velocity.jump.back.x)))
y = const(velocity.jump.y)

[State 40, 4];小ジャンプ（ジャンプ予備動作終了までに上を離す）
type = VelSet
trigger1 = var(26) = 0
trigger1 = AnimTime = 0
trigger1 = command != "holdup"
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x)-0.3, const(velocity.jump.back.x)+0.4))
y = const(velocity.jump.y)+3.15

[State 40, 4];中ジャンプ（大ジャンプコマンドを入力し、ジャンプ予備動作終了までに上を離す）
type = VelSet
trigger1 = var(26) = 3
trigger1 = AnimTime = 0
trigger1 = command != "holdup"
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x)+1, const(velocity.jump.back.x)))
y = const(velocity.jump.y)+3.15

[State 40, 4];大ジャンプ（大ジャンプコマンドを入れる）
type = VelSet
trigger1 = var(26) = 3
trigger1 = AnimTime = 0
trigger1 = command = "holdup"
x = ifelse(sysvar(1)=0, const(velocity.jump.neu.x), ifelse(sysvar(1)=1, const(velocity.jump.fwd.x)+1, const(velocity.jump.back.x)+0.3))
y = const(velocity.jump.y)-0.2

[State 40, 5]
type = VelSet
trigger1 = var(26) = 0
trigger1 = AnimTime = 0
trigger1 = command != "holdup"
trigger1 = prevstateno = 100 ;RUN_FWD
trigger1 = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)
y = const(velocity.jump.y)+3.15

[State 40, 5]
type = VelSet
trigger1 = var(26) = 0
trigger1 = command = "holdup"
trigger2 = var(26) = 3
triggerall = AnimTime = 0
triggerall = prevstateno = 100 ;RUN_FWD
triggerall = sysvar(1) = 1
x = const(velocity.runjump.fwd.x)

[State -3] ;←コマンドを入れたらVarをON
type = varset
trigger1 = AnimTime = 0
trigger1 = var(26) = 0
trigger1 = command != "holdup"
v = 26
value = 1

[State -3] ;←コマンドを入れたらVarをON
