; Jump Start
[Statedef 40]
type    = S
physics = S
anim = 40
ctrl = 0
sprpriority = 1
facep2 = 1

[State 0, ChangeState]
type = ChangeState
trigger1 = fvar(2) != 2 ;Not in Beast Style
trigger1 = var(59) != 0
trigger1 = time = 0
trigger1 = random <= 433
trigger1 = prevstateno != 4101
